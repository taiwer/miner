    "file-saver": "^2.0.5",
    "fuse": "^0.0.8",
    "fuse.js": "^6.4.3",
    "jquery": "^3.5.1",
    "js-cookie": "2.2.0",
    "jsonlint": "^1.6.3",
    "jszip": "^3.5.0",
